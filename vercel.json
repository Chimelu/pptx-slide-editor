{
  "functions": {
    "src/app/api/pptx/parse/route.ts": {
      "maxDuration": 60,
      "memory": 2048
    }
  },
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=2048"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=2048"
    }
  },
  "routes": [
    {
      "src": "/api/pptx/parse",
      "dest": "/api/pptx/parse",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
}
